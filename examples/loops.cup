fn main(): int {
    let sum1: int = 0;
    let sum2: int = 0;

    let N: int = 10;
    let i: int = 0;

    for (i = 0; i <= N; i = i + 1) {
        sum1 = sum1 + i;
    }

    i = 0;
    while (i <= N) {
        sum2 = sum2 + i;
        i = i + 1;
    }

    if (sum1 == sum2 && sum1 == 55) {
        return 0;
    }
    return 1;
}